@{
    Layout = null;
    var user = Session["UserC"] as CMS_DTO.CMSCustomer.CustomerModels;
}

<header id="header" class="header">
    <!-- top-header -->
    <div class="top-header">
        <!-- container -->
        <div class="container">
            <ul class="top-social col-md-6 col-sm-4">
                <li><a title="Facebook" href="#"><i class="fa fa-facebook"></i></a></li>
                <li><a title="Twitter" href="#"><i class="fa fa-twitter"></i></a></li>
                <li><a title="Google" href="#"><i class="fa fa-google-plus"></i></a></li>
                <li><a title="Pinterest" href="#"><i class="fa fa-pinterest-p"></i></a></li>
            </ul>
            <div class="col-md-6 ow-right-padding col-sm-8">
                <ul class="top-menu">

                    @if(user != null)
                    {
                        <li><a title="info" href="@Url.Action("Index","Profile")">@user.LastName</a></li>
                        <li><a title="Login" href="@Url.Action("Logout","Login")">Logout</a></li>
                    }
                    else
                    {
                        <li><a title="Login" href="@Url.Action("Index","Login")">Login</a></li>
                    }
                    <li><a title="Register" href="@Url.Action("Index","Register")">Register</a></li>
                </ul>
            </div>
        </div><!-- container /- -->
    </div><!-- top-header /- -->
